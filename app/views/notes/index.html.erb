<div id="notes">
  <script type="text/javascript">
    // notes for user <%= @user_id %>
    var notes = [];
    <% unless @note.nil? %>
      <% @note.each_index do |i| %>
      
      notes[<%= i %>] = {id: <%= @note[i].id %>, body: <%= @note[i].body.to_json %>, position_x: <%= @note[i].position_x || 50 %>, position_y: <%= @note[i].position_y || 50 %>, width: <%= @note[i].width || 50 %>, height: <%= @note[i].height || 50 %>};
      <% end %>
    <% end %>
  </script>
</div>
<div id="desktop"></div>
<h1>Notepad</h1>
<h2 id='click_to_start'>(click anywhere to get started)</h2>